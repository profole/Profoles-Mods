        <IsActiveConditional CurrentBrokenVolume="gt 0.1" targetitemcomponent="Engine" />
      </DecorativeSprite>
      <BrokenSprite texture="Content/Items/Engine/Engines.png" sourcerect="1372,0,659,317" depth="0.78" maxcondition="80" fadein="true" />
