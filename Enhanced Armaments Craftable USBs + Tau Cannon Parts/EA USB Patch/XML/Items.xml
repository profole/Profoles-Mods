      <Fabricate suitablefabricators="fabricator" requiredtime="30">
        <RequiredItem identifier="scp_prototypeusb" />
        <RequiredItem identifier="steel" amount="8" />
        <RequiredItem identifier="fulgurium" amount="8" />
        <RequiredItem identifier="dementonite" amount="8" />
        <RequiredItem identifier="physicorium" amount="8" />
      </Fabricate>
      <Deconstruct time="25">
        <Item identifier="fulgurium" amount="4" />
      </Deconstruct>
      <InventoryIcon name="taupartsinv" texture="%ModDir:2764968387%/Materials/materials.png" sourcerect="681,542,219,90" depth="0.55" origin="0.5,0.5" />
      <Sprite texture="%ModDir:2764968387%/Materials/materials.png" sourcerect="8,729,361,155" depth="0.55" origin="0.5,0.5" />
      <Body width="50" height="25" density="50" />
      <Holdable canBeCombined="true" removeOnCombined="true" slots="Any,RightHand,LeftHand" handle1="0,0" holdangle="90" msg="ItemMsgPickUpSelect">
        <StatusEffect type="OnBroken" target="This">
          <Remove />
        </StatusEffect>
      </Holdable>
      <AiTarget sightrange="1250" static="true" />
    </Item>
  </Items>
</Override>
